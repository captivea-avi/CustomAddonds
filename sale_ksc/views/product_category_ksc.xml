<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="product_uom_category_ksc_list_view" model="ir.ui.view">
            <field name="name">product.uom.category.ksc.list.view</field>
            <field name="model">product.uom.category.ksc</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
<!--                    <field name="parent_id"/>-->
                </tree>
            </field>
        </record>


        <record id="product_uom_category_ksc_form_view" model="ir.ui.view">
            <field name="name">product.uom.category.ksc.form.view</field>
            <field name="model">product.uom.category.ksc</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <sheet>
                            <field name="name"/>
<!--                            <field name="parent_id"/>-->
                        </sheet>
                    </group>
                </form>
            </field>
        </record>


        <record id="product_uom_category_ksc_search_view" model="ir.ui.view">
            <field name="name">product.uom.category.ksc.search.view</field>
            <field name="model">product.uom.category.ksc</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_my_sales_menu">
            <field name="name">My Sales</field>
            <field name="res_model">product.ksc</field>
            <field name="view_mode">tree,form,search</field>
        </record>

        <menuitem id="sale_ksc_menu" action="action_my_sales_menu"  name="Sale Ksc"/>
        <menuitem id="my_sale_ksc_menu"
                  action="action_my_sales_menu"
                  parent="sale_ksc_menu"
                  name="My Sales"/>
        <menuitem id="category_ksc_menu"
                  action="action_my_sales_menu"
                  parent="my_sale_ksc_menu"
                  name="Category"/>




    </data>
</odoo>